{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-start-dragging",
    "dialog:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://songbook.karaokecollective.com/*"
        }
      ]
    }
  ]
}